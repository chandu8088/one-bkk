<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Report Download"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <heading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                        fieldLabel="Heading"
                        name="./heading"
                        useFixedInlineToolbar="{Boolean}true">
                        <rtePlugins
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/onebangkok/components/authoring/richtext/rtePlugins"/>
                        <uiSettings
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/onebangkok/components/authoring/richtext/uiSettings"/>
                    </heading>
                    <description
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                        fieldLabel="Description"
                        name="./description"
                        useFixedInlineToolbar="{Boolean}true">
                        <rtePlugins
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/onebangkok/components/authoring/richtext/rtePlugins"/>
                        <uiSettings
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/onebangkok/components/authoring/richtext/uiSettings"/>
                    </description>
                    <assetrootpath
                        jcr:primaryType="nt:unstructured"
                        fieldDescription="Select a PDF from DAM"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldLabel="Asset Root path"
                        name="./assetrootPath"
                        required="{Boolean}true"
                        rootPath="/content/dam/onebangkok"/>
                    <filterheading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Filter heading"
                        name="./filterHeading"
                        required="{Boolean}true"/>
                    <filterdefaulttext
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Filter default text"
                        name="./filterDefaultText"
                        required="{Boolean}true"/>
                    <downloadctalabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Download Cta Label"
                        name="./downloadctaLabel"
                        required="{Boolean}true"/>
                    <noresultsmessage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                        fieldLabel="No results message"
                        name="./noresultsMessage"
                        required="{Boolean}true"
                        useFixedInlineToolbar="{Boolean}true">
                        <rtePlugins
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/onebangkok/components/authoring/richtext/rtePlugins"/>
                        <uiSettings
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/onebangkok/components/authoring/richtext/uiSettings"/>
                    </noresultsmessage>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
