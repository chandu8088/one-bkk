<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Global Footer"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[onebangkok.authoring.dialog]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="GENERAL"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <formid
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Subscription Form id"
                                                name="./formid"
                                                required="{Boolean}true"
                                                validation="onebkk-formid-validation"/>
                                            <mainheading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="3"
                                                text="Main Heading"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <heading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldLabel="Heading"
                                                        name="./heading"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/uiSettings"/>
                                                    </heading>
                                                    <description
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldLabel="Description"
                                                        name="./description"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/uiSettings"/>
                                                    </description>
                                                    <mainImg
                                                        jcr:primaryType="nt:unstructured"
                                                        fieldDescription="Aspect ratio 608 : 653, Use images with transparent background"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Main Image"
                                                        name="./mainImg"
                                                        rootPath="/content/dam/onebangkok"/>
                                                    <alttext
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Main Img Alt Text"
                                                        name="./mainLogoAltText"/>
                                                </items>
                                            </well>
                                            <copyrightheading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="3"
                                                text="Copyright"/>
                                            <Copy_x0020_of_x0020_well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <copyright
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Copyright"
                                                        name="./copyright"/>
                                                </items>
                                            </Copy_x0020_of_x0020_well>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                    <contact
                        jcr:primaryType="nt:unstructured"
                        jcr:title="CONTACT"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <brandsheading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="3"
                                                text="Brands Logo"/>
                                            <brands
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                validation="multifield-max-2">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./brandImgs">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/well">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <logo
                                                                    jcr:primaryType="nt:unstructured"
                                                                    fieldDescription="Aspect ratio 198 : 72, Use images with transparent background"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldLabel="Brand Logo"
                                                                    name="./image"
                                                                    rootPath="/content/dam/onebangkok"/>
                                                                <url
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldLabel="Brand Logo Url"
                                                                    name="./link"
                                                                    rootPath="/content/onebangkok"/>
                                                                <checkbox
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                    name="./openInNewTab"
                                                                    text="Open in New Tab"
                                                                    value="true"/>
                                                                <alttext
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Brands Logo Alt Text"
                                                                    name="./altText"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </field>
                                            </brands>
                                            <addressLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Address label"
                                                name="./addresslabel"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/uiSettings"/>
                                            </addressLabel>
                                            <addressDetails
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Address details"
                                                name="./addressdetails"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/richtext/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/richtext/uiSettings"/>
                                            </addressDetails>
                                            <contactLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Contact label"
                                                name="./contactlabel"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/uiSettings"/>
                                            </contactLabel>
                                            <contactnumber
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Contact number"
                                                name="./contactnumber"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/richtext/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/richtext/uiSettings"/>
                                            </contactnumber>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </contact>
                    <newsletter
                        jcr:primaryType="nt:unstructured"
                        jcr:title="NEWSLETTER SECTION"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Heading"
                                                name="./newsletterHeading"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/uiSettings"/>
                                            </heading>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Description"
                                                name="./newsletterDescription"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/onebangkok/components/authoring/rtev2/uiSettings"/>
                                            </description>
                                            <placeholdertxt
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Placeholder text"
                                                name="./placeholder"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </newsletter>
                    <footerlink
                        jcr:primaryType="nt:unstructured"
                        jcr:title="FOOTER LINK'S"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <footerlinks
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./footerLinks">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/well">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <label
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Label"
                                                                    name="./text"/>
                                                                <url
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldLabel="Link"
                                                                    name="./link"
                                                                    rootPath="/content/onebangkok"/>
                                                                <checkbox
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                    name="./openInNewTab"
                                                                    text="Open in New Tab"
                                                                    value="true"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </field>
                                            </footerlinks>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </footerlink>
                    <socialshare
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SOCIAL SHARE"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <socialshare
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                eaem-show-on-collapse="EAEM.showProductName">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./socialShare">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/well">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <logo
                                                                    jcr:primaryType="nt:unstructured"
                                                                    fieldDescription="Use svg icons"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldLabel="Social Media Logo"
                                                                    name="./image"
                                                                    rootPath="/content/dam/onebangkok"/>
                                                                <url
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldLabel="Social Media Link"
                                                                    name="./link"
                                                                    rootPath="/content/onebangkok"/>
                                                                <checkbox
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                    name="./openInNewTab"
                                                                    text="Open in New Tab"
                                                                    value="true"/>
                                                                <alttext
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Social Media Alt Text"
                                                                    name="./altText"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </field>
                                            </socialshare>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </socialshare>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
