<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[onebangkok.authoring.dialog]"
    mode="edit">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <generalDetails
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="General Details"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pinnedEvent
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        fieldLabel="Pinned Event"
                                                        name="./pinnedEvent"
                                                        text="Pinned event"
                                                        value="true"/>
                                                    <eventCategory
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                                        fieldLabel="Event category"
                                                        name="./eventCategory"
                                                        rootPath="/content/cq:tags/onebangkok"/>
                                                    <eventHeading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                        fieldLabel="Event Heading"
                                                        name="./eventHeading"
                                                        required="{Boolean}true"/>
                                                    <summary
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldLabel="Summary"
                                                        name="./summary"
                                                        required="{Boolean}true"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="onebangkok/components/authoring/richtext/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="onebangkok/components/authoring/richtext/uiSettings"/>
                                                    </summary>
                                                    <eventImage
                                                        jcr:primaryType="nt:unstructured"
                                                        fieldDescription="Aspect ratio 237 : 133"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Event imageÂ "
                                                        name="./eventImage"
                                                        required="{Boolean}true"
                                                        rootPath="/content/dam/onebangkok"/>
                                                    <alttext
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Image alt text"
                                                        name="./altText"/>
                                                    <thumbnailImage
                                                        jcr:primaryType="nt:unstructured"
                                                        fieldDescription="Aspect ratio 91 : 51"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Thumbnail image"
                                                        name="./thumbnailImage"
                                                        rootPath="/content/dam/onebangkok"/>
                                                    <thumbnailalttext
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Thumbnail  image alt text"
                                                        name="./thumbnailalttext"/>
                                                    <eventDescriptionHeading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldLabel="Event description heading"
                                                        name="./eventDescriptionHeading"
                                                        required="{Boolean}true"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="onebangkok/components/authoring/richtext/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="onebangkok/components/authoring/richtext/uiSettings"/>
                                                    </eventDescriptionHeading>
                                                    <eventDescription
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldLabel="Event description"
                                                        name="./eventDescription"
                                                        required="{Boolean}true"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="onebangkok/components/authoring/richtext/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="onebangkok/components/authoring/richtext/uiSettings"/>
                                                    </eventDescription>
                                                </items>
                                            </well>
                                        </items>
                                    </generalDetails>
                                </items>
                            </column>
                        </items>
                    </general>
                    <date
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Date"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <dateDetails
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Date Details"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <eventType
                                                        granite:class="cq-dialog-dropdown-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Event Type"
                                                        name="./eventtype">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <singleday
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Single Day"
                                                                value="singleday"/>
                                                            <multiday
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Multi Day"
                                                                value="multiday"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-dropdown-showhide-target=".showhidetargets"/>
                                                    </eventType>
                                                    <startdate
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                        fieldLabel="Start Date"
                                                        name="./startdate"
                                                        required="{Boolean}true"/>
                                                    <startTime
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                        fieldLabel="Start Time"
                                                        name="./startTime"
                                                        required="{Boolean}true"
                                                        type="time"/>
                                                    <endDate
                                                        granite:class="showhidetargets"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-dropdown-showhide-target=".showhidetargets"
                                                            showhidetargetvalue="multiday"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <enddate
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                                fieldLabel="End Date"
                                                                name="./enddate"/>
                                                        </items>
                                                    </endDate>
                                                    <endTime
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                        fieldLabel="End Time"
                                                        name="./endTime"
                                                        type="time"/>
                                                </items>
                                            </well>
                                        </items>
                                    </dateDetails>
                                </items>
                            </column>
                        </items>
                    </date>
                    <location
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Location"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <locationDetails
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Location Details"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <locationdetails
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldLabel="Location Details"
                                                        name="./locationdetails"
                                                        required="{Boolean}true"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="onebangkok/components/authoring/richtext/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="onebangkok/components/authoring/richtext/uiSettings"/>
                                                    </locationdetails>
                                                    <latitude
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Latitude"
                                                        name="./latitude"/>
                                                    <longitude
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Longitude"
                                                        name="./longitude"/>
                                                </items>
                                            </well>
                                        </items>
                                    </locationDetails>
                                </items>
                            </column>
                        </items>
                    </location>
                    <ticket
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Ticket"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <ticketDetails
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Ticket Details"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <ticketDetails
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldLabel="Ticket Details"
                                                        name="./ticketDetails"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="onebangkok/components/authoring/richtext/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="onebangkok/components/authoring/richtext/uiSettings"/>
                                                    </ticketDetails>
                                                    <ticketCTA
                                                        granite:class="cq-dialog-checkbox-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        fieldLabel="Enable Ticket CTA"
                                                        name="./enableTicketCTA"
                                                        text="Enable Ticket CTA"
                                                        value="true">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-checkbox-showhide-target=".togglefield"/>
                                                    </ticketCTA>
                                                    <ticketCTAtoggle
                                                        granite:class="togglefield"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <ticketCTAlabel
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                                fieldLabel="Ticket CTA label"
                                                                name="./ticketCtaLabel"
                                                                required="{Boolean}true"
                                                                useFixedInlineToolbar="{Boolean}true">
                                                                <rtePlugins
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceSuperType="onebangkok/components/authoring/richtextnoplugin/rtePlugins"/>
                                                                <uiSettings
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceSuperType="onebangkok/components/authoring/richtextnoplugin/uiSettings"/>
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    ticketlabel="ticketlabel"/>
                                                            </ticketCTAlabel>
                                                            <ticketCTAUrl
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldLabel="Ticket CTA URL"
                                                                name="./ticketCTAUrl"
                                                                rootPath="/content/onebangkok">
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    ticketlabel="ticketlabel"/>
                                                            </ticketCTAUrl>
                                                            <openinnewtab
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                name="./openinnewtab"
                                                                text="Open in new tab"
                                                                uncheckedValue="_self"
                                                                value="_blank">
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    ticketlabel="ticketlabel"/>
                                                            </openinnewtab>
                                                        </items>
                                                    </ticketCTAtoggle>
                                                </items>
                                            </well>
                                        </items>
                                    </ticketDetails>
                                </items>
                            </column>
                        </items>
                    </ticket>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
